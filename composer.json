{
    "name": "wppusher/casablanca",
    "description": "A framework for developing WordPress packages.",
    "keywords": [
        "wppusher",
        "casablanca",
        "wordpress"
    ],
    "homepage": "https://github.com/wppusher/Casablanca",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Suhm",
            "email": "peter@suhm.dk",
            "homepage": "http://petersuhm.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Casablanca\\": "src/Casablanca"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Casablanca\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
